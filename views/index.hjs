<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>{{ title }}</title>

  <meta name="description" content="Description.">
  <meta name="keywords" content="">
  <meta name="author" content="">
  <meta name="viewport" content="initial-scale=1.0">

  <!-- START Social media metas -->

  <!-- Google Authorship and Publisher Markup 
  <link rel="author" href="https://plus.google.com/[Google+_Profile]/posts">
  <link rel="publisher" href="https://plus.google.com/[Google+_Page_Profile]">
  -->

  <!-- Open graph 
  <meta property="og:title" content="Title">
  <meta property="og:description" content="Description">
  <meta property="og:type" content="website">
  <meta property="og:url" content="">
  <meta property="og:image" content="">
  -->

  <!-- Twitter Card data
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:site" content="@publisher_handle">
  <meta name="twitter:title" content="Page Title">
  <meta name="twitter:description" content="Page description less than 200 characters">
  <meta name="twitter:creator" content="@author_handle">
  -->
  <!-- Twitter summary card with large image must be at least 280x150px 
  <meta name="twitter:image:src" content="http://www.example.com/image.html">
  -->

  <!-- END Social media metas -->

  <!-- CSS -->
  <link href="assets/style.css" rel="stylesheet"> 

  <!-- fonts, glyphicons -->
  <link href='http://fonts.googleapis.com/css?family=Droid+Sans:400,700' rel='stylesheet' type='text/css'>
  <link href="/assets/bootstrap-glyphicons.css" rel="stylesheet">
  
  <!-- jQuery and UI -->
  <script src="assets/jquery-1.10.1.min.js"></script>
  <script src="assets/jquery-ui-1.10.4/js/jquery-ui-1.10.4.min.js"></script>
  <link href="assets/jquery-ui-1.10.4/css/ui-lightness/jquery-ui-1.10.4.min.css" rel="stylesheet">

  <!-- fancyBox image gallery -->
  <link rel="stylesheet" href="assets/fancybox/source/jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />
  <script type="text/javascript" src="assets/fancybox/source/jquery.fancybox.pack.js?v=2.1.5"></script>

  <!-- Optionally add helpers - button, thumbnail and/or media -->
  <link rel="stylesheet" href="assets/fancybox/source/helpers/jquery.fancybox-thumbs.css?v=1.0.7" type="text/css" media="screen" />
  <script type="text/javascript" src="assets/fancybox/source/helpers/jquery.fancybox-thumbs.js?v=1.0.7"></script>

  <!-- fancyBox helpers not used 
  <link rel="stylesheet" href="assets/fancybox/source/helpers/jquery.fancybox-buttons.css?v=1.0.5" type="text/css" media="screen" />
  <script type="text/javascript" src="assets/fancybox/source/helpers/jquery.fancybox-buttons.js?v=1.0.5"></script>
  <script type="text/javascript" src="assets/fancybox/source/helpers/jquery.fancybox-media.js?v=1.0.6"></script>
  -->

  <!-- HTML5 shiv for below IE10 compatibility -->
  <!--[if lt IE 9]>
  <script src="assets/html5shiv-printshiv.min.js"></script>
  <script src="assets/respond.min.js"></script>
  <![endif]-->

</head>


<body>

<!-- HTML5 structural block -->

<!-- nav responsive controlled by a condenser script below -->
<header class="col span_all" id="main_header">
    <div id="header" class="global_wrapper">
      <img src="images/logo3.png" id="logo">
      <!-- inline css to force <HTML5 compatibility -->
      <nav style="display:inline;" id="nav_normal"> 
        <span class="nav_buttons nav_selected">Home</span>
        <span class="nav_buttons">Following</span>
        <span class="nav_buttons">Followers</span>
        <span class="nav_buttons">Explore</span>
      </nav>
      <span class="nav_search_field"><input type="text" id="nav_search_input"><span class="glyphicon glyphicon-search"></span></span>
    </div>
    <nav style="display:inline;" id="nav_condensed">
      <span class="nav_buttons"><span class="glyphicon glyphicon-th-list"></span></span>
      <span class="nav_buttons_condensed selected">Home</span>
      <span class="nav_buttons_condensed">Following</span>
      <span class="nav_buttons_condensed">Followers</span>
      <span class="nav_buttons_condensed">Explore</span>
    </nav>
</header>

<div class="global_wrapper header_compensate">

  <section id="left_col" class="col span_2">
    <div class="block_title">
        <h1>What's <span id="word_hot">Hot</span></h1>
    </div>

    <!-- start hot stream -->

    <div class="left_col_block">
      <article id="trend_001">
        <div class="block_top block_inner">
          <div class="block_user_holder"><img src="images/bloggerpics/blogger_Vivian.jpg">
          </div>
          <div class="block_text_holder">
            <div class="block_username">Vivian liked</div>
            <div class="block_timelapse">2 days ago</div>
          </div>
        </div>
        <a rel="group" class="fancybox" href="images/dress2.jpg">
          <img src="images/dress2.jpg" width="100%">
        </a>
        <header class="block_inner">
          <h2 class="item_name" itemprop="item">Black dress</h2>
          <h2 class="shop_name" itemprop="shop">Love Bonito</h2>
          <h3 class="price" itemprop="price">$89.00</h3>
          <p>#black #lovebonito</p>
          <p>982 <span class="glyphicon glyphicon-heart"></span>, 203 <span class="glyphicon glyphicon-share-alt"></span>, 2 comments</p>
          <p><span class="glyphicon glyphicon-heart"></span> I want this! | <span class="glyphicon glyphicon-share-alt"></span> Share</p>
        </header>
      </article>
    </div>

    <!-- end hot stream -->
  </section><!-- left_col -->

  <section id="main" class="col span_1">
      <div class="block_title">
        <h1>Your <span id="word_stream">Stream</span></h1>
      </div>

    <!-- start your stream -->

    <div class="main_col_block">
      <article id="stream_001">
        <div class="block_top block_inner">
          <div class="block_user_holder"><img src="images/bloggerpics/blogger_Verlyn.jpg">
          </div>
          <div class="block_text_holder">
            <div class="block_username">Verlyn liked</div>
            <div class="block_timelapse">4 minutes ago</div>
          </div>
        </div>
        <a rel="group" class="fancybox" href="images/dress1.jpg">
          <img src="images/dress1.jpg" width="100%">
        </a>
        <header class="block_inner">
          <h2 class="item_name" itemprop="item">Pink dress</h2>
          <h2 class="shop_name" itemprop="shop">Klarra</h2>
          <h3 class="price" itemprop="price">$95.00</h3>
          <p>#pink #pinkdress #klarra #under100</p>
          <p>31 <span class="glyphicon glyphicon-heart"></span>, 12 <span class="glyphicon glyphicon-share-alt"></span>, 2 comments</p>
          <p><span class="glyphicon glyphicon-heart"></span> I want this! | <span class="glyphicon glyphicon-share-alt"></span> Share</p>
        </header>
      </article>

      <article id="stream_002">
        <div class="block_top block_inner">
          <div class="block_user_holder"><img src="images/bloggerpics/blogger_Verlyn.jpg">
          </div>
          <div class="block_text_holder">
            <div class="block_username">Verlyn liked</div>
            <div class="block_timelapse">30 minutes ago</div>
          </div>
        </div>
        <a rel="group" class="fancybox" href="images/dress3.jpg">
          <img src="images/dress3.jpg" width="100%">
        </a>
        <header class="block_inner">
          <h2 class="item_name" itemprop="item">White dress</h2>
          <h2 class="shop_name" itemprop="shop">Kiss Jane</h2>
          <h3 class="price" itemprop="price">$34.00</h3>
          <p>#white #dress #kissjane</p>
          <p>10 <span class="glyphicon glyphicon-heart"></span>, 5 <span class="glyphicon glyphicon-share-alt"></span>, 1 comments</p>
          <p><span class="glyphicon glyphicon-heart"></span> I want this! | <span class="glyphicon glyphicon-share-alt"></span> Share</p>
        </header>
      </article>
    </div>

    <!-- end your stream -->
  </section><!-- main -->

  <aside id="right_col" class="col span_3">
    <div class="right_menu block_inner">
      <h2>{{ someVariable }}</h2>
      <p>1) jQuery 1.10.1</p>
      <p>2) jQuery UI 1.10.4</p>
      <p>3) fancyBox 2.1.5</p>
      <p>4) respond.js</p>
      <p>5) HTML5 shiv</p>           
    </div>
  </aside><!-- right_col -->
</div><!-- .global_wrapper -->

  <footer id="footer" class="col span_all">
    <div>
      <h1>This is the footer.</h1>         
      <p></p>
    </div>
  </footer>




  <!-- All JS goes here -->
  <!-- Bootstrap JS 
  <script src="assets/bootstrap-3.1.1/js/bootstrap.min.js"></script>-->
  <!--<script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>-->

  <!-- fancyBox init -->
  <script>
  $( document ).ready( function () {
    $(".fancybox").fancybox();
  });
  </script>

  <!-- Gridpak *FOR DEV ONLY*
  <script src="assets/gridpak.js"></script>-->
  <script>
  $(document).keyup(function(e) {
    if (e.keyCode == 71) {
      $( 'div' ).css('outline', '1px dotted red');
      $( 'span' ).css('outline', '1px dotted red');
    }
    if (e.keyCode == 72) {
      $( 'div' ).css('outline', '0px');
      $( 'span' ).css('outline', '0px');
    }
  });
  </script>

  <!-- condensed navigation -->
  <script>
    var x = $( 'nav' ).html();
  </script>
</body>
</html>
